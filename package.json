{
  "name": "odd-notes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@strictmode/eslint-config": "^0.5.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.28.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": ["@strictmode/eslint-config"],
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    }
  }
}
